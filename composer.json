{
    "name": "ebethus/laravel-s3-cache-driver",
    "description": "Laravel S3 cache driver. Support: Amazon S3, Digital Ocean Spaces, etc.",
    "type": "library",
    "homepage": "https://github.com/EBethus/laravel-s3-driver-cache",
    "keywords":
    [
        "php",
        "laravel",
        "laravel cache driver",
        "s3",
        "amazon s3",
        "digital ocean spaces"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "imannms",
            "email": "socialtrens@gmail.com",
            "homepage": "https://github.com/imannms",
            "role": "Developer"
        },
        {
            "name": "Alberto Berroteran Sanchez",
            "email": "alberroteran@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": ">= 8.0",
        "league/flysystem-aws-s3-v3": "^3.13.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.5"
    },
    "autoload": {
        "psr-4": {
            "EBethus\\LaravelS3CacheDriver\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "EBethus\\LaravelS3CacheDriver\\S3CacheServiceProvider"
            ]
        }
    }
}